#include <iostream>
#include <string>
using namespace std;

int main(){
  float a = 1.0f;
  cout << (int)a << endl;
  cout << &a << endl;
  cout << (int&)a << endl;
  float b = 0.0f;
  cout << (int)b << endl;
  cout << &b << endl;
  cout << (int&)b <<endl;
  int c = 1;
  cout << &c << endl;

  unsigned char a = 0xA5;
  unsigned char b=~a>>4+1;
  
  cout << b;
  return 0;
}
